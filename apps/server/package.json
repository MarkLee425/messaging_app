{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "scripts": {
    "start": "nodemon src/index.ts",
    "build": "rimraf dist && tsc",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "jest --detectOpenHandles"
  },
  "jest": {
    "preset": "@repo/jest-presets/jest/node"
  },
  "dependencies": {
    "@repo/logger": "workspace:*",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/jest-presets": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.5",
    "ts-node-dev": "^2.0.0",
    "cookie-parser": "^1.4.6",
    "supertest": "^6.3.3",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
